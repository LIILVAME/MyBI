{
  "name": "doogoo",
  "version": "0.2.2",
  "description": "Smart Property Monitoring & Analytics Platform",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run i18n:compile && vite build",
    "preview": "vite preview",
    "test:unit": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write \"src/**/*.{js,ts,vue,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,vue,json,css}\"",
    "audit:lighthouse": "node scripts/run-lighthouse.js",
    "audit:vercel": "node scripts/audit-vercel-env.js",
    "audit:supabase": "node scripts/test-supabase-connectivity.js",
    "audit:health": "node scripts/generate-health-report.js",
    "audit:all": "npm run audit:vercel && npm run audit:supabase && npm run audit:health",
    "lint:i18n": "node scripts/validate-i18n.js",
    "test:i18n": "node scripts/test-i18n-ci.js",
    "i18n:compile": "node scripts/compile-translations.js",
    "i18n:build": "npm run i18n:compile && npm run build",
    "pwa:icons": "node scripts/generate-pwa-icons.js",
    "pwa:validate": "./scripts/validate-pwa.sh",
    "email:audit": "node scripts/audit-email-templates.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@lingui/cli": "^5.5.2",
    "@lingui/core": "^5.5.2",
    "@lingui/macro": "^5.5.2",
    "@sentry/tracing": "^7.120.4",
    "@sentry/vue": "^10.22.0",
    "@supabase/supabase-js": "^2.78.0",
    "@vueuse/core": "^14.0.0",
    "apexcharts": "^5.3.5",
    "file-saver": "^2.0.5",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue3-apexcharts": "^1.10.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.6",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.39.0",
    "eslint-plugin-vue": "^10.5.1",
    "glob": "^11.0.3",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.6",
    "node-fetch": "^2.7.0",
    "postcss": "^8.4.38",
    "prettier": "^3.6.2",
    "sharp": "^0.34.4",
    "supabase": "^2.54.11",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^5.2.0",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^4.0.6"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
